<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.0//EN"
                           "http://java.sun.com/dtd/slee-event-jar_1_0.dtd">
<event-jar>
    <event-definition>
        <description>call in initialization, initialzes media</description>
        <event-type-name>org.mobicents.slee.service.interopdemo.INIT_MEDIA</event-type-name>
        <event-type-vendor>org.mobicents</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.service.events.InteropCustomEvent</event-class-name>
    </event-definition>

    <event-definition>
        <description>Call setup, play announcement</description>
        <event-type-name>org.mobicents.slee.service.interopdemo.PLAY_ANN_OPENING</event-type-name>
        <event-type-vendor>org.mobicents</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.service.events.InteropCustomEvent</event-class-name>
    </event-definition>
    
    <event-definition>
        <description>Call setup, play announcement</description>
        <event-type-name>org.mobicents.slee.service.interopdemo.PLAY_ANN_CONFIRM</event-type-name>
        <event-type-vendor>org.mobicents</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.service.events.InteropCustomEvent</event-class-name>
    </event-definition>
    
    <event-definition>
        <description>Call ended, terminates media</description>
        <event-type-name>org.mobicents.slee.service.interopdemo.END_MEDIA</event-type-name>
        <event-type-vendor>org.mobicents</event-type-vendor>
        <event-type-version>1.0</event-type-version>
        <event-class-name>org.mobicents.slee.service.events.InteropCustomEvent</event-class-name>
    </event-definition>                          
</event-jar>
