	entity="sip:someone@example.com">

	<tuple id="x8eg92m">
		<status>
			<basic>closed</basic>
		</status>
		<rp:user-input>idle</rp:user-input>
		<rp:class>auth-1</rp:class>
		<contact priority="0.5">sip:user@example.com</contact>
